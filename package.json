    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-storybook": "^0.6.4",
    "husky": "^8.0.1",
    "jest": "^29.1.1",
    "jest-environment-jsdom": "^29.1.1",
    "lint-staged": ">=10",
    "npm-run-all": "^4.1.5",
